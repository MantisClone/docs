    "gatsby-remark-github": "^2.0.0",
    "gatsby-remark-images": "^4.2.0",
    "gatsby-remark-responsive-iframe": "^3.2.0",
    "gatsby-remark-smartypants": "^3.2.0",
    "gatsby-remark-vscode": "^3.3.1",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-source-git": "^1.1.0",
    "gatsby-source-graphql": "^3.14.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "gatsby-transformer-xml": "^2.10.0",
    "gatsby-transformer-yaml": "^3.14.0",
    "giphy-js-sdk-core": "^1.0.6",
    "intersection-observer": "^0.12.0",
    "js-search": "^2.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-json-view": "^1.21.3",
    "react-scrollspy": "^3.4.3",
    "rehype-react": "^7.0.4",
    "remark": "^13.0.0",
    "remark-github-plugin": "^1.4.0",
    "remark-react": "^8.0.0",
    "sass": "^1.51.0",
    "shortid": "^2.2.16",
    "slugify": "^1.6.5",
    "smoothscroll-polyfill": "^0.4.4",
    "swagger-client": "^3.18.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oceanprotocol": "^1.5.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-format-staged": "^3.0.0",
    "husky": "^7.0.4",
    "markdownlint-cli": "^0.31.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "webpack": "^5.72.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oceanprotocol/docs"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
